import java.util.*;
class Decimaltobinary
{
	public static void main(String args[])
	{
		int binary[] = new int[1000];
		int i=0,n;
		Scanner s = new Scanner(System.in);
		System.out.println("Enter the number= ");
		n = s.nextInt();
		while(n>0)
		{
			binary[i]=n%2;
			n=n/2;
			i++;
		}
		for(i=i-1;i>=0;i--)
		{
			System.out.print(binary[i]);
		}
	}
}
