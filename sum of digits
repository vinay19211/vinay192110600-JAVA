import java.util.*;
class Practice
{
 	public static void main(String args[])
	{
		int n,i,sum=0,rem;
		Scanner s = new Scanner(System.in);
		System.out.println("Enter the number= ");
		n = s.nextInt();
		while(n>0)
		{
			rem=n%10;
			sum=sum+rem;
			n=n/10;
		}
	System.out.println("The sum of N digits number= "+sum);	
		
	}
}
