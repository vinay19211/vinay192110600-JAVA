class Parent
{
	public void method(){
		System.out.println("This is parent class");
	}
}
class Child extends Parent
{
	public void method1(){
		System.out.println("This is child class");
	}
	public static void main(String args[])
	{
		Parent obj = new Parent();
		Child obj1 = new Child();
		obj.method();
		obj1.method1();
		obj1.method();
	}
}
