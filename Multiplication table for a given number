import java.util.*;
class Practice
{
	public static void main(String args[])
	{
		int m,n,i;
		Scanner s = new Scanner(System.in);
		System.out.println("Enter the m value= ");
		m = s.nextInt();
		System.out.println("Enter the n value= ");
		n = s.nextInt();
		for(i=1;i<=n;i++)
		{
			System.out.println(i+"*"+m+"="+i*m);
		}
	}
	
}
