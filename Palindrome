import java.util.*;
class Test
{
	public static void main(String args[])
	{
		int n,temp,rem,sum=0;
		Scanner s = new Scanner(System.in);
		System.out.println("Enter the number=");
		n = s.nextInt();
		temp=n;
		while(n>0)
		{
			rem=n%10;
			sum=sum*10+rem;
			n=n/10;
		}
		if(temp==sum)
		{
			System.out.println("Palindrome");
		}
		else
		{
			System.out.println("Not a Palindrome");
		}
	}
}
