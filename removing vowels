import java.util.*;
class Remove_vowels
{
	public static void main(String args[])
	{
		int len=0,i;
		String s1;
		char ch;
		Scanner s=new Scanner(System.in);
		System.out.println("enter the string: ");
		s1=s.nextLine();
		len=s1.length();
		for(i=0;i<len;i++)
		{
			ch=s1.charAt(i);	
			if(ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u')
			{
				continue;
			}
			else
			{
				System.out.print(ch);
			}
		}
	}
}
