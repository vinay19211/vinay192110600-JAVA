import java.util.*;
class Test
{
	public static void main(String args[])
	{
		int a,b;
		Scanner s = new Scanner(System.in);
		System.out.println("Enter the num1= ");
		a = s.nextInt();
		System.out.println("Enter the num2=");
		b=s.nextInt();
		a=a+b;
		b=a-b;
		a=a-b;
		System.out.println("a="+a);
		System.out.println("b="+b);
		
	}
}
