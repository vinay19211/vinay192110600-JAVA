import java.util.*;
class Righttraingle{
	public static void main(String args[])
	{
		int n,i,j,num=1;
		Scanner s = new Scanner(System.in);
		System.out.println("Enter the no of rows=");
		n = s.nextInt();
		for(i=1;i<=n;i++)
		{
			for(j=1;j<=i;j++)
			{
				System.out.print(num*num+" ");
				num++;
			}
			System.out.println();
		}
	}
}
