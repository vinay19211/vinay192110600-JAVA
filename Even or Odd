import java.util.*;
class Test
{
	public static void main(String args[])
	{
		int num,i;
		Scanner s = new Scanner(System.in);
		System.out.println("Enter the value=");
		num = s.nextInt();
		if(num%2==0)
		{
			System.out.println("Even Number");
		}
		else
		{
			System.out.println("Odd Number");
		}
	}
}
