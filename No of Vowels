import java.util.*;
class Vowelscount{
	public static void main(String args[])
	{
		String s1;
		int vowels=0,i;
		Scanner s = new Scanner(System.in);
		System.out.println("Enter the string=");
		s1=s.nextLine();
		int n = s1.length();
		for(i=0;i<n;i++)
		{
			char ch=s1.charAt(i);
			if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U')
			{
				vowels++;
			}
		}
		System.out.println("The no of vowels="+vowels);
	}
}
